import java.util.Scanner;
class Main
{
public static void main (String[] args)
{
    int i=2,ans=0;
    Scanner in=new Scanner(System.in);
    int n=in.nextInt();
    for(i=2;i<=n/2;i++)
    {
         ans=n%i;
    }
        if(ans==0)
        System.out.println("Not a prime number\n");
        else 
        System.out.println("Prime Number");
    }
    
}
